#. Description
#: ../ubiquity.templates:30001
msgid "Full Name:"
msgstr "전체 이름:"

#. Type: text
#. Description
#: ../ubiquity.templates:31001
msgid "Login Name:"
msgstr "로그인 이름:"

#. Type: text
#. Description
#: ../ubiquity.templates:32001
