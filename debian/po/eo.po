#. Description
#: ../ubiquity.templates:242
msgid "Installing system"
msgstr "Instalanta sistemon"

#. Type: text
#. Description
#: ../ubiquity.templates:246
msgid "Finding the distribution to copy..."
msgstr "Trovanta la disdonaĵon por kopii..."

#. Type: text
#. Description
#: ../ubiquity.templates:250
msgid "Scanning files..."
msgstr "Skananta dosierojn..."

#. Type: text
#. Description
#: ../ubiquity.templates:254
msgid "Copying files..."
msgstr "Kopianta dosierojn..."

#. Type: text
#. Description
#: ../ubiquity.templates:258
msgid "Copying files (${TIME} remaining)..."
msgstr "Kopianta dosierojn (${TIME} restas)..."

#. Type: text
#. Description
#: ../ubiquity.templates:262
msgid "Copying installation logs..."
msgstr "Kopianta instaladajn registraĵojn..."

#. Type: text
#. Description
#: ../ubiquity.templates:266
msgid "Cleaning up..."
msgstr "Purigi ..."

#. Type: text
#. Description
#: ../ubiquity.templates:270
msgid "Running ${SCRIPT}..."
msgstr "Interpretado de ${SCRIPT} ..."

#. Type: text
#. Description
#: ../ubiquity.templates:274
msgid "Configuring system locales..."
msgstr "Akomodanta sistemajn lokarojn..."

#. Type: text
#. Description
#: ../ubiquity.templates:278
msgid "Configuring apt..."
msgstr ""

#. Type: text
#. Description
#: ../ubiquity.templates:282
msgid "Configuring time zone..."
msgstr "Akomodanta horan zonon..."

#. Type: text
#. Description
#: ../ubiquity.templates:286
msgid "Configuring keyboard..."
msgstr ""

#. Type: text
#. Description
#: ../ubiquity.templates:290
msgid "Creating user..."
msgstr "Kreanta uzanton..."

#. Type: text
#. Description
#: ../ubiquity.templates:294
msgid "Configuring hardware..."
msgstr "Akomodi aparataĵon..."

#. Type: text
#. Description
#: ../ubiquity.templates:298
msgid "Configuring network..."
msgstr "Akomodi reton ..."

#. Type: text
#. Description
#: ../ubiquity.templates:302
msgid "Setting computer name..."
msgstr "Fiksanta komputilan nomon..."

#. Type: text
#. Description
#: ../ubiquity.templates:306
msgid "Configuring boot loader..."
msgstr "Akomodanta iniciatan ŝargilon..."

#. Type: text
#. Description
#: ../ubiquity.templates:310
msgid "Removing extra packages..."
msgstr "Foriganta ekstrajn pakojn..."

#. Type: text
#. Description
#: ../ubiquity.templates:314
msgid "Checking for packages to remove..."
msgstr "Kontrolanta kiujn ajn pakojn por forigi..."

#. Type: text
#. Description
#: ../ubiquity.templates:318
msgid "Downloading packages (${TIME} remaining)..."
msgstr "Elŝutanta pakojn (${TIME} restanta)..."

#. Type: text
#. Description
#: ../ubiquity.templates:322
msgid "Downloading package lists..."
msgstr ""

#. Type: text
#. Description
#: ../ubiquity.templates:326
msgid "Downloading package lists (${TIME} remaining)..."
msgstr "Elŝutanta pakajn listojn (${TIME} restanta)..."

#. Type: error
#. Description
#: ../ubiquity.templates:335
msgid "Error installing ${PACKAGE}"
msgstr "Eraris instalante ${PACKAGE}"

#. Type: error
#. Description
#: ../ubiquity.templates:340
msgid "Error removing ${PACKAGE}"
msgstr "Eraris forigante ${PACKAGE}"

#. Type: error
#. Description
