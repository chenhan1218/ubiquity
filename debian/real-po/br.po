msgid "Edit a partition"
msgstr "Kemmañ ur parzhad"

#. Type: text
#. Description
#: ../ubiquity.templates:86001
msgid "Advanced Options"
msgstr "Dibarzhioù kempleshoc'h"

#. Type: text
#. Description
#: ../ubiquity.templates:87001
msgid "Boot loader"
msgstr "Karger loc'hañ"

#. Type: text
#. Description
#: ../ubiquity.templates:88001
msgid "Install boot loader"
msgstr "Staliañ ar c'harger loc'hañ"

#. Type: text
#. Description
#: ../ubiquity.templates:89001
msgid "Popularity contest"
msgstr "Konkour brud vat"

#. Type: text
#. Description
#: ../ubiquity.templates:90001
msgid "Participate in the package usage survey"
msgstr "Kemer perzh er studiadenn eus arver ar pakad"

#. Type: text
#. Description
#: ../ubiquity.templates:91001
msgid "Network proxy"
msgstr "Proksi ar rouedad"

#. Type: text
#. Description
